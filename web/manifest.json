{
  "name": "Pluto",
  "description": "A beautiful new tab for your browser.",
  "version": "0.3.0",
  "manifest_version": 3,
  "short_name": "Pluto",
  "content_security_policy": {
    "content_security_policy": {
      "extension_pages": "script-src 'self' ; object-src 'self'"
    }
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "icon128.png"
  }
}